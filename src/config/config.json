{
    "config": {
        "database": {
            "host": "localhost",
            "user": "root",
            "pwd": "root",
            "db_name": "beer_craft_shop_database"
        },
        "installed": "true",
        "storefront": "off",
        "fast_mode": "false"
    },
    "tables": {
        "products": {
            "id": "INT(11) UNSIGNED AUTO_INCREMENT PRIMARY KEY",
            "name": "VARCHAR(35) CHARACTER SET utf8 NOT NULL",
            "description": "TEXT",
            "price": "DECIMAL(10, 2) NOT NULL",
            "img_url": "VARCHAR(125)",
            "percentage": "DECIMAL(10, 2)"
        },
        "users": {
            "id": "INT(11) UNSIGNED AUTO_INCREMENT PRIMARY KEY",
            "firstname": "VARCHAR(35) CHARACTER SET utf8 NOT NULL",
            "lastname": "VARCHAR(35) CHARACTER SET utf8 NOT NULL",
            "email": "VARCHAR(35) CHARACTER SET utf8 NOT NULL",
            "password": "VARCHAR(255) NOT NULL"
        },
        "admins": {
            "id": "INT(11) UNSIGNED AUTO_INCREMENT PRIMARY KEY",
            "login_name": "VARCHAR(35) CHARACTER SET utf8 NOT NULL",
            "password": "VARCHAR(255) NOT NULL"
        }
    }
}